e_settings></tab_browse><tab_sql><sql name="SQL 1">SELECT SOP.SALE_ORDER_PRODUCT_ID, SOP.SALE_ORDER_PRODUCT, SUM(SOP.SALE_ORDER_PRODUCT_QUANTITY) AS TOTAL_QUANTITY
FROM SALE_ORDER_PRODUCT SOP
JOIN SALE_ORDER SO ON SOP.SALE_ORDER_ID = SO.SALE_ORDER_ID
WHERE SO.SALE_ORDER_STATUS &lt;&gt; 'Delivered'
GROUP BY SOP.SALE_ORDER_PRODUCT_ID, SOP.SALE_ORDER_PRODUCT;</sql><current_tab id="0"/></tab_sql></sqlb_project>
